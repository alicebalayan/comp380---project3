{% extends "base.jinja" %}
{% block title %}{{ title }}{% endblock %}
{% block content %}
<main>
    <section id="sideBar">
        <div class="d-flex menuBtn collapseBtn">
            <h5 class="p-2 m-0"><i class="fas fa-bars m-2"></i><strong><span>Pane title</span></strong></h5>
        </div>
        <form class="navbar-form navbar-right" role="search" style="width: 100%;">
            <div class="form-group searchBoxSide">
                <input type="search" class="form-control" placeholder="Search">
            </div>
            <button type="submit" class="btn btn-default"></button>
        </form>
        <a href="/dashboard">
            <div class="d-flex menuBtn">
                <h6 class="p-2 m-0"><i class="fas fa-home m-2"></i> <span>Dashboard<span></span></span></h6>
            </div>
        </a>
        <div class="panel-group mt-5" id="accordion">
            <div>
                <a data-toggle="collapse" data-parent="#accordion" href="#projects" class="collapsed"
                    aria-expanded="false">
                    <div class="menuBtn dropDownMenu">
                        <h6 class="p-2 m-0 d-flex"><i class="fa fa-tasks  m-2" aria-hidden="true"></i>
                            <strong><span class="dropDown">

                                    Projects <i class="fas fa-chevron-down m-2"></i>
                                </span></strong></h6>
                    </div>
                </a>
                <div id="projects" class="panel-collapse collapse">
                    <div class="d-flex menuBtn pl-5 current">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                1<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                2<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                3<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                4<span></span></span></h6>
                    </div>
                </div>
            </div>
            <div>
                <a data-toggle="collapse" data-parent="#accordion" href="#reports" class="collapsed"
                    aria-expanded="false">
                    <div class="menuBtn dropDownMenu">
                        <h6 class="p-2 m-0 d-flex"><i class="fa fa-bug  m-2" aria-hidden="true"></i>
                            <strong><span class="dropDown">

                                    Reports <i class="fas fa-chevron-down m-2"></i>
                                </span></strong></h6>
                    </div>
                </a>
                <div id="reports" class="panel-collapse collapse">
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                1<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                2<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                3<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                4<span></span></span></h6>
                    </div>
                </div>
            </div>
            <div>
                <a data-toggle="collapse" data-parent="#accordion" href="#help" class="collapsed" aria-expanded="false">
                    <div class="menuBtn dropDownMenu">
                        <h6 class="p-2 m-0 d-flex"><i class="fa fa-question-circle  m-2" aria-hidden="true"></i>
                            <strong><span class="dropDown">

                                    Help System <i class="fas fa-chevron-down m-2"></i>
                                </span></strong></h6>
                    </div>
                </a>
                <div id="help" class="panel-collapse collapse">
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                1<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                2<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                3<span></span></span></h6>
                    </div>
                    <div class="d-flex menuBtn pl-5">
                        <h6 class="p-2 m-0"><i class="fas fa-project-diagram m-2"></i> <span>Project
                                4<span></span></span></h6>
                    </div>
                </div>
            </div>

        </div>
        <div>
            <a href="/settings">
                <div class="d-flex menuBtn settings mt-5">
                    <h6 class="p-2 m-0"><i class="fas fa-cog m-2"></i> <span>Settings<span></span></span></h6>
                </div>
            </a>
        </div>
    </section>
    <section id="mainContent">
        <section class="ml-3 mt-0">
            <article>
                <h1><strong>Project Name placeholder</strong></h1>
                <nav>
                    <ul class="d-flex p-0">
                        <li class="mr-4 {% if 'Deliverables'==page%}current{% endif %}"><a
                                href="deliverables">Deliverables</a></li>
                        <li class="mr-4 {% if 'Tasks'==page%}current{% endif %}"><a href="tasks">Tasks</a></li>
                        <li class="mr-4 {% if 'Issues'==page%}current{% endif %}"><a href="issues">Issues</a></li>
                        <li class="mr-4 {% if 'Action Items'==page%}current{% endif %}"><a href="actionItems">Action
                                Items</a></li>
                        <li class="mr-4 {% if 'Decisions'==page%}current{% endif %}"><a href="decisions">Decisions</a>
                        </li>
                        <li class="mr-4 {% if 'Resources'==page%}current{% endif %}"><a href="resources">Resources</a>
                        </li>
                        <li class="mr-4 {% if 'Risks'==page%}current{% endif %}"><a href="risks">Risks</a></li>
                        <li class="mr-4 {% if 'Requirments'==page%}current{% endif %}"><a
                                href="requirments">Requirments</a></li>
                        <li class="mr-4 {% if 'Changes'==page%}current{% endif %}"><a href="changes">Changes</a></li>
                        <li class="mr-4 {% if 'Reference Documents'==page%}current{% endif %}"><a
                                href="referenceDocuments">Reference Documents</a></li>
                        <li class="mr-4 {% if 'Components'==page%}current{% endif %}"><a
                                href="components">Components</a></li>
                        <li class="mr-4 {% if 'Defects'==page%}current{% endif %}"><a href="defects">Defects</a></li>
                    </ul>
                </nav>
            </article>
            <article class="mainContent d-flex">
                <section>
                </section>
                <section class="mr-5">
                    <a class="btn btn-default" id="createbtn" href="javascript:create();" data-url="/create{{page[:-1]}}">Create {{page[:-1]}}</a>
                    <div class="form-group">
                        <H5>Sort</H5>
                        <select multiple class="form-control">
                            <option>Lorem</option>
                            <option>ipsum</option>
                            <option>dolor</option>
                            <option>sit</option>
                            <option>amet</option>
                        </select>
                    </div>
                    <H5>Filter by date</H5>
                    <input type="date" id="filterDate" class="form-control" placeholder="mm/dd/yyyy">

                </section>
            </article>

        </section>
        {{self.footer()}}
    </section>
    <div class="modal" id="modal-sample" tabindex="-1" role="dialog" aria-labelledby="modal-sample-label" aria-hidden="true">
        
    </div>

</main>
{% endblock %}